    "from bs4 import BeautifulSoup\n",
    "import urllib3\n",
    "\n",
    "url = \"https://www.bing.com\"\n",
    "\n",
    "def antiHTML(url):\n",
    "    response = urllib.request.urlopen(url).read()\n",
    "    soup = BeautifulSoup(response,'html.parser')\n",
    "    strips = list(soup.stripped_strings)\n",
    "    return strips\n",
    "\n",
    "print(antiHTML(url))"
